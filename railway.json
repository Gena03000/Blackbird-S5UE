{
  "build": {
    "builder": "docker"
  },
  "deploy": {
    "startCommand": "gunicorn app:app --bind 0.0.0.0:$PORT"

    "restartPolicy": "always",
    "healthcheck": {
      "path": "/",
      "interval": 10000,
      "timeout": 3000,
      "retries": 5
    }
  },
  "env": {
    "NODE_ENV": "postgres:17",
"PORT": "5000"
  }
}
